{
  "name": "theme-to-zed",
  "displayName": "Theme to Zed",
  "description": "Export VSCode themes to Zed",
  "version": "0.0.4",
  "publisher": "degreat",
  "icon": "assets/vz.png",
  "repository": "https://github.com/blackmann/theme-to-zed.git",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "theme-to-zed.export",
        "title": "Export Theme to Zed"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Theme to Zed",
      "properties": {
        "theme-to-zed.theme_importer": {
          "type": "string",
          "description": "Path to the theme_importer executable"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
